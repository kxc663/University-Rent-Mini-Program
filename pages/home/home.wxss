.float-button {
  position: fixed;
  bottom: 5vh;
  right: 5vw;
  width: 10vh;
  height: 10vh;
  background-color: #001aff;
  border-radius: 50%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.float-button-text {
  color: #ffffff;
  font-size: 30px;
}